Install:
  go get -u github.com/vexilology/shorteninGO

Support:
  - [!] Cutt.ly
  - [x] Tinyurl.com

== Available with Cutt.ly
  [!] - Create
  [!] - Analytic
  [x] - Change name(subscribe mode)
  [x] - Delete link(subscribe mode)
  [x] - Edit link/title/name(subscribe mode)

== Exampe Cutt.ly
package main

import "fmt"

func conf() *ConfigurationCuttly { return Add("PRIVATE_KEY") }

func main() {
  c := conf()
  cc, err := c.CuttlyNew("YOUR_LINK")
  if err != nil {
    fmt.Println(err)
    return
  }
  fmt.Println(cc.Url.Status, cc.Url.ShortLink, cc.Url.Title)

  cs, err := c.CuttlyAnalytics("YOUR_SHORT")
  if err != nil {
    fmt.Println(err)
    return
  }
  fmt.Println(cs.Stats.Date, cs.Stats.Clicks, cs.Stats.Title)
}